{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "BeeAI",
  "colors": {
    "primary": "#000000",
    "light": "#c5c5c5",
    "dark": "#000000"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "groups": [
      {
        "group": "Introduction",
        "pages": [
          "/introduction/welcome",
          "/introduction/quickstart",
          "/introduction/installation"
        ]
      },
      {
        "group": "Getting Started",
        "pages": [
          "/getting-started/discover-agents",
          "/getting-started/environment-configuration",
          "/getting-started/run-agents",
          "/getting-started/manage-agents",
          "/getting-started/share-agents",
          "/getting-started/cli-reference",
          "/getting-started/ui-overview"
        ]
      },
      {
        "group": "Core Concepts",
        "pages": ["concepts/architecture",
                  "concepts/acp-protocol",
                  "concepts/agents"]
      },
      {
        "group": "Building Agents",
        "pages": ["building-agents/build-custom-agents",
                  "building-agents/bring-in-external-agents"]
      },
      {
        "group": "Deployment",
        "pages": ["deployment/self-hosting-options",
                  "deployment/kubernetes"
                 ]
      },
      {
        "group": "Community and Support",
        "pages": ["community-and-support/troubleshooting",
                  "community-and-support/contribute"]
      }
    ]
  },
  "logo": {
    "light": "/logo/beeai-lockup.svg",
    "dark": "/logo/beeai-lockup-white.svg"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "GitHub",
      "href": "https://github.com/i-am-bee/beeai"
    },
    "links": [
      {
        "label": "Agent Catalog",
        "href": "https://beeai.dev/agents"
      },
      { "label": "Discord", "href": "https://discord.gg/NradeA6ZNF" }
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/i-am-bee/beeai",
      "discord": "https://discord.gg/AZFrp3UF5k",
      "bluesky": "https://bsky.app/profile/beeaiagents.bsky.social",
      "youtube": "https://www.youtube.com/@BeeAIAgents"
    }
  },
  "redirects": [
    {
      "source": "/acp/spec/openapi.yaml",
      "destination": "https://github.com/i-am-bee/acp/blob/main/docs/spec/openapi.yaml"
    },
    {
      "source": "/acp/:slug*",
      "destination": "https://agentcommunicationprotocol.dev"
    }
  ]
}
